<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Akış | Projenerasyon</title>
    <link
      rel="icon"
      type="image/png"
      sizes="16x16"
      href="assets/img/favicon.png"
    />
    <link rel="stylesheet" href="assets/css/animate.css" />
    <link rel="stylesheet" href="assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="assets/css/line-awesome.min.css" />
    <link rel="stylesheet" href="assets/css/jquery.mCustomScrollbar.min.css" />
    <link rel="stylesheet" href="assets/css/all.min.css" />
    <link rel="stylesheet" href="assets/css/fontawesome.min.css" />
    <link rel="stylesheet" href="assets/css/slick.css" />
    <link rel="stylesheet" href="assets/css/slick-theme.css" />
    <link rel="stylesheet" href="assets/css/style.css" />
    <link rel="stylesheet" href="assets/css/responsive.css" />
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.min.js"
      integrity="sha384-Atwg2Pkwv9vp0ygtn1JAojH0nYbwNJLPhwyoVbhoPwBhjQPR5VtM2+xf0Uwh9KtT"
      crossorigin="anonymous"
    ></script>
  </head>

  <body>
    <header>
      <div class="container">
        <div class="header-data">
          <div class="logo">
            <a href="index.html" title=""
              ><img src="assets/img/logo.png" alt=""
            /></a>
          </div>
          <div class="search-bar">
            <form>
              <input type="text" name="search" placeholder="Search..." />
              <button type="submit"><i class="la la-search"></i></button>
            </form>
          </div>
          <nav>
            <ul>
              <li>
                <a href="/action" title="" class="not-box-openm">
                  <span><img src="assets/img/baglantilar2.png" alt="" /></span>
                  Bağlantılarım
                </a>
              </li>

              <li>
                <a href="#" title="" class="not-box-open">
                  <span><img src="assets/img/is-ortaklarim.png" alt="" /></span>
                  İş Ortaklarım
                </a>
              </li>
              <li>
                <a href="/chat" title="" class="not-box-openm">
                  <span><img src="assets/img/mesaj.png" alt="" /></span>
                  Mesajlaşma
                </a>
              </li>
              <li>
                <a href="notifications.html" title="" class="not-box-openm">
                  <span><img src="assets/img/bildirim.png" alt="" /></span>
                  Bildirimler
                </a>
              </li>
              <div class="user-account">
                <% user.forEach(users => { %>
                <div class="user-info">
                  <img src="assets/img/avatar.png" alt="" />
                  <a href="#" title=""
                    ><%= users.name %> <%= users.lastname %></a
                  >
                  <i class="la la-sort-down"></i>
                </div>
                <% }) %>
                <div class="user-account-settingss" id="users">
                  <h3>Hesabım</h3>
                  <ul class="us-links">
                    <li><a href="/profile" title="">Profilim</a></li>
                    <li><a href="#" title="">Bilgilerim</a></li>
                    <li><a href="#" title="">Blog Yazılarım</a></li>
                    <li><a href="#" title="">Şirket Künyem</a></li>
                    <li><a href="#" title="">Çözümlerim</a></li>
                    <li><a href="#" title="">Pazar Bilgilerim</a></li>
                  </ul>
                  <h3 class="tc">
                    <a href="/user/logout" title="">Oturumu Kapat</a>
                  </h3>
                </div>
              </div>
              <li>
                <a href="" title="">
                  <span><img src="assets/img/operasyonlar.png" alt="" /></span>
                  Operasyon<i class="la la-sort-down"></i>
                </a>
                <ul>
                  <li>
                    <ul class="menu-alt">
                      <h3>
                        <a href="" title=""
                          ><i class="las la-street-view"></i> Aksiyonlarım</a
                        >
                      </h3>
                    </ul>
                  </li>

                  <li>
                    <ul class="menu-alt">
                      <h3>
                        <a href="" title=""
                          ><i class="lab la-product-hunt"></i> Projelerim</a
                        >
                      </h3>
                    </ul>
                  </li>
                  <li>
                    <ul class="menu-alt">
                      <h3>
                        <a href="" title=""
                          ><i class="las la-user-secret"></i> İş
                          İstihbaratlarım</a
                        >
                      </h3>
                    </ul>
                  </li>
                </ul>
              </li>
              <li>
                <a href="" title="">
                  <span><img src="assets/img/servis.png" alt="" /></span>
                  Servis<i class="la la-sort-down"></i>
                </a>
                <ul class="service">
                  <li>
                    <ul class="menu-alt">
                      <h3>
                        <i class="las la-briefcase"></i> İş Geliştirme Servisi
                      </h3>
                      <ul class="us-links">
                        <li>
                          <a href="" title=""
                            ><i class="las la-calendar"></i> Randevu Servisi</a
                          >
                        </li>
                        <li>
                          <a href="" title=""
                            ><i class="las la-user-shield"></i> Danışman
                            Servisi</a
                          >
                        </li>
                      </ul>
                    </ul>
                  </li>

                  <li>
                    <ul class="menu-alt">
                      <h3>
                        <i class="las la-bullhorn"></i> Bilgilendirme Servisi
                      </h3>
                      <ul class="us-links">
                        <li>
                          <a href="" title=""
                            ><i class="las la-ad"></i> Reklam Servisi</a
                          >
                        </li>
                        <li>
                          <a href="" title=""
                            ><i class="las la-percent"></i> Kampanya Servisi</a
                          >
                        </li>
                      </ul>
                    </ul>
                  </li>
                  <li>
                    <ul class="menu-alt">
                      <h3>
                        <a href="" title=""
                          ><i class="las la-globe"></i> İhracat Servisi</a
                        >
                      </h3>
                    </ul>
                  </li>
                </ul>
              </li>
            </ul>
          </nav>
          <div class="user-account mobil">
            <% user.forEach(users => { %>
            <div class="user-info">
              <img src="assets/img/avatar.png" alt="" />
              <a href="#" title=""><%=users.name %> <%=users.lastname %></a>
              <i class="la la-sort-down"></i>
            </div>
            <% }) %>
            <div class="user-account-settingss" id="users">
              <h3>Hesabım</h3>
              <ul class="us-links">
                <li><a href="#" title="">Profilim</a></li>
                <li><a href="#" title="">Bilgilerim</a></li>
                <li><a href="#" title="">Blog Yazılarım</a></li>
                <li><a href="#" title="">Şirket Künyem</a></li>
                <li><a href="#" title="">Çözümlerim</a></li>
                <li><a href="#" title="">Pazar Bilgilerim</a></li>
              </ul>
              <h3 class="tc">
                <a href="/user/logout" title="">Oturumu Kapat</a>
              </h3>
            </div>
          </div>
          <div class="menu-btn">
            <a href="#" title=""><i class="fa fa-bars"></i></a>
          </div>
        </div>
      </div>
    </header>
  </body>
</html>
